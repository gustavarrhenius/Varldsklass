@using Varldsklass.Domain.Helpers
@model IEnumerable<Varldsklass.Domain.Entities.Event>
<div id="calendar">
<h4>Kalender<i>&#128197;</i></h4>
<ul class="calendar">
    
    @foreach (var item in Model)
    {
        
            <li>
                <h5>@Html.ActionLink(@item.Post.Title, "EventSingle", "Course", new { id = item.ID}, null)</h5>
                <i class="clock">&#128340;</i><span class="calendar-date">@Html.DateFormat(item.StartDate) - @Html.DateFormat(item.EndDate)</span><br />
                <i>&#59172;</i>@item.City
            </li>
    }
  </ul>   
</div>
